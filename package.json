{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": {
    "electron-icon-generator": "dist/index.js"
  },
  "dependencies": {
    "commander": "9.3.0",
    "fs-extra": "10.1.0",
    "icon-gen": "3.0.1",
    "jimp": "0.16.1"
  },
  "description": "An icon generator to generate all the icon files needed for electron packaging",
  "devDependencies": {
    "@ffflorian/eslint-config": "0.8.2",
    "@ffflorian/prettier-config": "0.1.3",
    "@ffflorian/semantic-release-config": "0.5.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-no-unsanitized": "4.0.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "1.1.5",
    "husky": "4.3.8",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "publish-flat": "1.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.3",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ffflorian/electron-icon-generator",
  "keywords": [
    "electron",
    "icon",
    "icns",
    "png",
    "ico"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "@ffflorian/electron-icon-generator",
  "prettier": "@ffflorian/prettier-config",
  "release": {
    "extends": "@ffflorian/semantic-release-config"
  },
  "repository": "https://github.com/ffflorian/electron-icon-generator.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "flatten": "publish-flat -o flattened",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ext .ts src/",
    "prettier": "prettier \"*.{json,md}\"",
    "release": "semantic-release",
    "start": "ts-node src/cli.ts",
    "test": "exit 0"
  },
  "version": "1.0.1"
}
